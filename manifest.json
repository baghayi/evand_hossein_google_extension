{
    "name": "Evand Hossein",
    "version": "1.0",
    "description": "This extension is supposed to make Hossein's life much much easier.",
    "permissions": ["https://api.evand.com/", "tabs", "storage", "https://evand.com/"],
    "browser_action": {
    },
    "background": {
        "scripts": [
            "tasks/DisplayTotalUnreadMessages.js",
            "tasks/StartingScreenSelector.js",
            "tasks/EventWebinarLoginBox/BackgroundTask.js"
        ],
        "persistent": false
    },
    "manifest_version": 2,
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "linux": "Alt+C",
                "windows": "Alt+C"
            }
        }
    }
}
